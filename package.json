{
  "name": "@borekb/gatsby-starter-mobx",
  "description": "Gatsby starter with MobX and TypeScript",
  "version": "1.0.0",
  "author": "Borek Bernard <borekb@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "tsc": "tsc",
    "lint": "tslint -p .",
    "prettier": "prettier --write 'src/**'",
    "gatsby": "gatsby",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@theme-ui/presets": "^0.3.0",
    "@types/theme-ui": "^0.3.1",
    "animejs": "^3.2.0",
    "babel-preset-gatsby": "^0.3.1",
    "eslint-plugin-react-hooks": "^3.0.0",
    "gatsby": "^2.20.10",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-plugin-typescript": "^2.3.1",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.8",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-transition-group": "^4.3.0",
    "theme-ui": "^0.3.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/react-test-renderer": "^16.9.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.6",
    "prettier": "2.0.2",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^25.3.0",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*\\.([tj]sx?)|(\\.|/)(test|spec))\\.([tj]sx?)$",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/__mocks__/fileMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "setupFiles": [
      "<rootDir>/jest/loadershim.js"
    ]
  }
}
